<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yue.directrabbitmqsender.mapper.MyUserDao">
    <insert id="insertUser" parameterType="com.yue.directrabbitmqsender.entity.MyUser" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
        insert into my_user
        (name,age) values (#{myUser.name},#{myUser.age})
    </insert>
</mapper>